public void println()
public void prints(string i)
public void printi(integer i)

public integer entry (integer argc, string *argv, string *envp) {
	integer i;
	integer j;
	i := 2;
	j := 3
	if (i > 1 & i < 5) then {
		prints("in if\n");
	}
	else {
		prints("in else\n");
	}
	do {
		j:= 3;
		do {
			prints("j -> ")
			printi(++j)
			println();
		}while (j < 10)
		prints("i -> ")
		printi(++i)
		println()
	}while (i < 3)

	prints("not in if\n");
	entry := 0;
};
